  "pages": {
    "insulina": {
      "title": "Cálculo de Insulina | Calculadoras de Enfermagem Profissional",
      "description": "Calculadora profissional para aspiração de insulina em seringas de 100UI e 300UI. Segurança do paciente com 9 certos e auditoria de cálculo clínica.",
      "keywords": "insulina, enfermagem, cálculo de medicação, segurança do paciente, seringa insulina, auditoria clínica",
      "canonical": "/insulina",
      "ogImage": "/assets/images/author-info.webp",
      "header": {
        "badge": "Segurança Clínica",
        "title": "Cálculo de Insulina",
        "description": "Ferramenta profissional para aspiração precisa em seringas de 100UI ou 300UI."
      },
      "calculator": {
        "fields": [
          {
            "id": "prescricao",
            "label": "Prescrição Médica (UI)",
            "type": "number",
            "placeholder": "Ex: 15",
            "required": true,
            "min": 0,
            "step": 0.5,
            "tooltip": "Quantidade de insulina prescrita pelo médico em unidades internacionais (UI)."
          },
          {
            "id": "concentracao",
            "label": "Concentração da Insulina (UI/mL)",
            "type": "select",
            "options": [
              { "value": 100, "label": "100 UI/mL (Insulina Regular U100)" },
              { "value": 200, "label": "200 UI/mL (Insulina Glargine U200)" },
              { "value": 300, "label": "300 UI/mL (Insulina Degludec U300)" }
            ],
            "default": 100,
            "tooltip": "Concentração da insulina. Padrão: 100 UI/mL."
          }
        ],
        "formula": "Volume (mL) = Prescrição (UI) / Concentração (UI/mL)"
      },
      "auditSteps": [
        { "label": "Prescrição Médica", "icon": "fa-file-medical" },
        { "label": "Concentração da Insulina", "icon": "fa-vial" },
        { "label": "Tipo de Seringa", "value": "Seringa de Insulina 1mL", "icon": "fa-syringe" },
        { "label": "Fator de Cálculo", "icon": "fa-calculator" },
        { "label": "Volume Final Auditado", "icon": "fa-check-double" }
      ],
      "checklists": {
        "noveCertos": [
          "Paciente Certo",
          "Medicação Certa",
